{
  "expo": {
    "name": "Marcha Marlo",
    "slug": "marcha-marlo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "splash": {
      "image": "./assets/splash_with_logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#F1F1F1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marchamarlo.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.marchamarlo.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}